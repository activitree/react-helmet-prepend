{
    "presets": [
        ["@babel/preset-env", {
            "targets": {
                "browsers": [ ">0.2%", "not dead"]
            },
            "modules": false,
            "loose": true
        }],
        "@babel/preset-react"
    ],
    "plugins": [
        "transform-class-properties",
        "transform-object-rest-spread"
    ]
}
