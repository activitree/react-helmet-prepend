{
    "presets": [
        ["@babel/preset-env", {
            "targets": {
                "browsers": ["last 1 versions", "ie >= 10"]
            },
            "modules": false,
            "loose": true
        }],
        "@babel/preset-react"
    ],
    "plugins": [
        "transform-class-properties",
        "transform-object-rest-spread"
    ]
}
